{
  "name": "ali-monitor-juridico",
  "version": "1.0.0",
  "description": "Monitor jurídico sem banco de dados para acompanhamento de processos",
  "main": "src/server.ts",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc",
    "serve": "node dist/src/server.js",
    "start": "node dist/src/server.js",
    "build-tsc": "tsc",
    "clean": "rimraf dist",
    "worker": "tsx scripts/worker.ts",
    "worker:build": "node dist/scripts/worker.js",
    "netlify:dev": "netlify dev",
    "netlify:build": "echo ok"
  },
  "keywords": [
    "juridico",
    "monitor",
    "processos",
    "typescript",
    "express",
    "netlify"
