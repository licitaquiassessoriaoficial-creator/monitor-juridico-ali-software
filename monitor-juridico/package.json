  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "@octokit/rest": "^21.0.0",
    "@netlify/functions": "^2.8.0",
    "jsonwebtoken": "^9.0.2",
    "netlify-cli": "^17.0.0",
    "nodemailer": "^6.9.8",
    "@types/nodemailer": "^6.4.14"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.0",
    "@types/jsonwebtoken": "^9.0.3",
    "rimraf": "^5.0.5"
  },
  "packageManager": "pnpm@10.17.0"
